<%- include ('partials/header') %>
<title><%- (typeof(user) !== 'undefined')? user.name : A %>'s Dashboard </title>



	<link rel="stylesheet" type="text/css" href="/css/student_dashboard.css">
<body class="overlay-scrollbar">
	<!-- navbar -->
	<%- include ('partials/navbar.ejs') %>
	<%- include ('partials/teacher_sidebar.ejs') %>
	<!-- main content -->
	<div class="wrapper">
		<div class="row">
			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-primary">
					<p>
						<i class="fas fa-tasks"></i>
					</p>
					<h3>1</h3>
					<p>Assignments To do</p>
				</div>
			</div>

			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-warning">
					<p>
						<i class="fas fa-spinner"></i>
					</p>
					<h3>100+</h3>
					<p>In Evaluation</p>
				</div>
			</div>

			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-success">
					<p>
						<i class="fas fa-check-circle"></i>
					</p>
					<h3>100+</h3>
					<p>Evaluated</p>
				</div>
			</div>

			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-danger">
					<p>
						<i class="fas fa-bug"></i>
					</p>
					<h3>1</h3>
					<p>Results</p>
				</div>
			</div>

		</div>

		<!-- This is table part code  -->
		<div class="row">
			<div class="col-8 col-m-12 col-sm-12">
				<div class="card">
					<div class="card-header">
						<h3>
							Assignments
						</h3>
						<i class="fas fa-ellipsis-h"></i>
					</div>
					<div class="card-content">
						<table>
							<thead>
								<!-- add the logic of assignments here -->
								<tr>
									<th>S. No.</th>
									<th>Name</th>
									<th>Course</th>
									<th>Due date</th>
								</tr>
							</thead>
							<tbody>
								
							</tbody>
						</table>
					</div>
				</div>
			</div>


			<!-- this is progress bar code  -->
			<div class="col-4 col-m-12 col-sm-12">
				<div class="card">
					<div class="card-header">
						<h3>
							Progress bar
						</h3>
						<i class="fas fa-ellipsis-h"></i>
					</div>
					<div class="card-content">
						<div class="progress-wrapper">
							<p>
								Test Score
								<span class="float-right">50%</span>
							</p>
							<div class="progress">
								<div class="bg-success" style="width: 50%"></div>
							</div>
						</div>
						<div class="progress-wrapper">
							<p>
								Overall Attendence
								<span class="float-right">60%</span>
							</p>
							<div class="progress">
								<div class="bg-primary" style="width:60%"></div>
							</div>
						</div>

	<!-- <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js"></script> -->
	<script type="text/javascript" src="js/student_dashboard.js"></script>
	<!-- end import script -->
	<%- include ('partials/footer.ejs') %>